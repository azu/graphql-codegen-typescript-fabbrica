{
  "name": "graphql-fabbrica",
  "description": "GraphQL Code Generator Plugin to define mock data factory.",
  "version": "0.0.0",
  "type": "commonjs",
  "sideEffects": false,
  "repository": "https://github.com/mizdra/app_name.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": true,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "run-s -c lint:*",
    "lint:tsc": "tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "test": "vitest"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "devDependencies": {
    "@mizdra/eslint-config-mizdra": "^2.0.0",
    "@mizdra/prettier-config-mizdra": "^1.0.0",
    "@tsconfig/node18": "^18.2.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.5.1",
    "eslint": "^8.47.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [],
  "files": [
    "src",
    "!src/test",
    "!src/**/*.test.ts",
    "!src/**/__snapshots__",
    "dist"
